{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": [
          "last 2 versions",
          "not dead",
          "not < 2%",
          "not ie 11",
          "maintained node versions"
        ],
        "useBuiltIns": "entry",
        "corejs": "3.16.0"
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": ["react-hot-loader/babel"]
}
